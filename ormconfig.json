{
   "type": "sqlite",
   "database": "bin/database.sqlite",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/db/schema/**/*.ts"
   ],
   "migrations": [
      "src/db/migration/**/*.ts"
   ],
   "subscribers": [
      "src/db/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/db/schema",
      "migrationsDir": "src/db/migration",
      "subscribersDir": "src/db/subscriber"
   }
}